    "publish:win32": "butler push ./build/presentation.game-win32-ia32/ icbat/presentation-game:windows-32 --userversion npm_package_version",
    "publish:win64": "butler push ./build/presentation.game-win32-x64/ icbat/presentation-game:windows-6 --userversion npm_package_version4"
